{
  "compilerOptions": {
    "jsx": "react",
    "strictNullChecks": false,
    "types": ["types.d.ts", "./data/*.d.ts"],
    "suppressImplicitAnyIndexErrors": false,
    "noImplicitAny": false,
    "plugins": [
      {
        "name": "typescript-deno-plugin",
        "enable": true,
        "importmap": "import_map.json"
      }
    ]
  }
}
